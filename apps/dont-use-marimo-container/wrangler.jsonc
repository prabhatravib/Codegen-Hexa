{
  "name": "codegen-marimo-v2",
  "main": "src/index.ts",
  "compatibility_date": "2025-08-22",
  "observability": { "enabled": true },
  "durable_objects": {
    "bindings": [{ "name": "MARIMO", "class_name": "MarimoContainer" }]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_classes": ["MarimoContainer"]
    }
  ],
  "containers": [
    {
      "class_name": "MarimoContainer",
      "image": "./Dockerfile",
      "max_instances": 10
    }
  ]
}
